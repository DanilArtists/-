#include <iostream>
#include <string>

using namespace std;

class Konst {
private:
    int intLog;
    double doubleCop;
    string stringEding;

public:
    Konst() 
    {
        intLog = 0;
        doubleCop = 0.0;
        stringEding = "This";
    }

    void printKonst() const {
        cout << "IntLog: " << intLog << endl;
        cout << "DoubleCop: " << doubleCop << endl;
        cout << "StringEding: " << stringEding << endl;
    }
        
};

int main() {
    Konst rabota1(5, 3.0, "Правило");
    cout << "rabota1:" << endl;
    rabota1.printKonst();
    cout << endl;

    Konst rabota2 = rabota1;
    cout << "rabota2:" << endl;
    rabota2.printKonst();
    cout << endl;

}
